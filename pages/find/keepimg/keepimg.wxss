/* pages/find/keepimg/keepimg.wxss */
.keepimg_view{
  background-color:rgb(240,240,240);
  height:50px;
  padding:10px 20px;
  margin-bottom: 10px;
  display:flex;
  align-items:center;
  /* justify-content: center; */
}
.list{
  /* box-shadow:2px 2px 2px #00000045; */
  padding:5px;
}
.img_item {
  width: 100%;
  display: inline-block;
  vertical-align: top;
  height:auto;
}
.batch{
  position:fixed;
  height:50px;
  width:100%;
  z-index:999;
}
.checkbox{
  position:absolute;
}
.modal_view{
  width:100%;
  height:40px;
  line-height:40px;
  font-size:20px;
  color:#000;
  text-align:center;
}
.edit_name_btn{
  width: 100%;
  height: 49%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.edit_name_btn2{
  width: 100%;
  height: 35%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.click{
  position: relative;
  z-index: 99999;
  background-color: #fff;
}
.lately_keep2{
  width: 70px;
  height: 70px;
  border-radius: 5px;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #fff;
}
.click_end{
  position: '';
  z-index: 0;
}