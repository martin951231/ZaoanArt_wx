/**index.wxss**/
.search_input{
  width:95%;
}
.index_box{
  display:inline-block;
  text-align: center;
  width: 50%;
  position:relative;
}
.cate_view{
  margin: 10px 0px;
  width: 100%;
  height:30px;
  line-height: 30px;
  text-align: center;
  color:#fff;
  background-color: #000;
}
.theme_view{
  margin: 10px 0px;
  width: 100%;
  text-align: center;
  height:30px;
  line-height: 30px;
  color:#fff;
  background-color: #000;
}
.cate_img_text{
  position:absolute;
  left:25px;
  bottom:25px;
  background-color:#60606099;
  border-radius:2px;
  padding:2px 5px;
  color:#fff;
}
.theme_img_text{
  position:absolute;
  left:25px;
  bottom:25px;
  background-color:#60606099;
  border-radius:2px;
  padding:2px 5px;
  color:#fff;
}
.cate_img{
  width: 95%;
}
.theme_img{
  width: 95%;
}

